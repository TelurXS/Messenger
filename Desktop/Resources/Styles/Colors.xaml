<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LighterPrimary">#424549</Color>
    <Color x:Key="LightPrimary">#4a4c51</Color>
    <Color x:Key="Primary">#36393e</Color>
    <Color x:Key="DarkPrimary">#2e3136</Color>
    <Color x:Key="DarkerPrimary">#1e2124</Color>

    <Color x:Key="Text">White</Color>

    <SolidColorBrush x:Key="LighterPrimaryBrush" Color="{StaticResource LighterPrimary}"></SolidColorBrush>
    <SolidColorBrush x:Key="LightPrimaryBrush" Color="{StaticResource LightPrimary}"></SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkPrimaryBrush" Color="{StaticResource DarkPrimary}"></SolidColorBrush>
    <SolidColorBrush x:Key="DarkerPrimaryBrush" Color="{StaticResource DarkerPrimary}"></SolidColorBrush>
    
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource Text}"></SolidColorBrush>
    
</ResourceDictionary>